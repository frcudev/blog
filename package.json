{
  "name": "frcu-dev",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "lint": "ng lint",
    "lint-staged": "lint-staged",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:blog": "ng test --project=blog",
    "test:blog:watch": "ng test --project=blog --watch",
    "e2e": "ng e2e",
    "format:test": "prettier projects/**/*.{ts,json,md,scss} --list-different",
    "format:write": "prettier projects/**/*.{ts,json,md,scss} --write",
    "analyze:blog": "ng build --prod --stats-json && webpack-bundle-analyzer ./dist/blog/stats-es2015.json",
    "commit": "git add . && npx git-cz",
    "commit:select": "npx git-cz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.6",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "~9.0.6",
    "@angular/compiler": "~9.0.6",
    "@angular/core": "~9.0.6",
    "@angular/forms": "~9.0.6",
    "@angular/material": "^9.1.3",
    "@angular/platform-browser": "~9.0.6",
    "@angular/platform-browser-dynamic": "~9.0.6",
    "@angular/router": "~9.0.6",
    "@briebug/jest-schematic": "^2.1.1",
    "bulma": "^0.8.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^9.0.0",
    "@angular-devkit/build-angular": "~0.900.6",
    "@angular/cli": "~9.0.6",
    "@angular/compiler-cli": "~9.0.6",
    "@angular/language-service": "~9.0.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-angular": "^8.3.4",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "25.1.4",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.1",
    "cz-customizable": "^6.2.0",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "25.1.0",
    "jest-preset-angular": "8.1.2",
    "js-beautify": "^1.10.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.3",
    "stylelint": "^13.2.1",
    "stylelint-order": "^4.0.0",
    "ts-node": "~8.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5",
    "webpack-bundle-analyzer": "^3.6.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "ng lint && pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test --maxWorkers=8 && ng build"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ]
  },
  "jest": {
    "preset": "jest-preset-angular",
    "transform": {
      "^.+\\.(ts|js|html)$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup-jest.ts"
    ],
    "moduleNameMapper": {
      "@blog/app/(.*)": "<rootDir>/projects/blog/src/app/$1",
      "@blog/assets/(.*)": "<rootDir>/projects/blog/src/assets/$1",
      "@blog/env": "<rootDir>/projects/blog/src/environments/environment"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    }
  }
}
